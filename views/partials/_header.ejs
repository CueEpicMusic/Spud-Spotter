<!-- Header for Wiki-Map -->

<head>
  <title>Spud Spotters</title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
  <link rel="stylesheet" href="/styles/nav.css" type="text/css" />
  <link rel="stylesheet" href="/styles/maps.css" type="text/css" />

  <script defer type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>

  <script defer type="text/javascript" src="/scripts/app.js"></script>

  <link href="http://fonts.cdnfonts.com/css/clearviewhwy" rel="stylesheet">

</head>
<header>

  <nav class="navbar-top">
    <span class="nav-link">
      <a href="/" class="nav-links">Spud Spotters</a>
    </span>

    <% if(!userId){ %>

      <span class="nav-links"><a href="/login">Login</a></span>
      <span class="nav-links"><a href="/register">Register</a></span>

      <% } else{ %>

        <span class="nav-links"><a href="/maps">My Maps</a></span>
        <span class="nav-links"><a href="/maps/favourites">Favourites</a></span>
        <span class="nav-links"><a href="/maps/create">Create Maps</a></span>

        <form action="/logout" method="POST">
          <label for="logout">Logged In As: <%= userName %> </label>
          <button class="nav-links" type="submit">LogOut</button>
        </form>

        <% } %>
  </nav>
</header>
<div class="layout">
